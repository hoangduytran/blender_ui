msgstr "Số mẫu vật dọc theo trục x của thể tích"

#. :src: bpy.types.LightProbeVolume.resolution_y
#. :src: bpy.types.RenderSettings.resolution_y
#: source/blender/nodes/geometry/nodes/node_geo_volume_cube.cc:41
msgid "Resolution Y"
msgstr "Độ Phân Giải Y"

#. :src: bpy.types.LightProbeVolume.resolution_y
msgid "Number of samples along the y axis of the volume"
msgstr "Số mẫu vật dọc theo trục y của thể tích"

#. :src: bpy.types.LightProbeVolume.resolution_z
#: source/blender/nodes/geometry/nodes/node_geo_volume_cube.cc:45
msgid "Resolution Z"
msgstr "Độ Phân Giải Z"

#. :src: bpy.types.LightProbeVolume.resolution_z
msgid "Number of samples along the z axis of the volume"
msgstr "Số mẫu vật dọc theo trục z của thể tích"

#. :src: bpy.types.LightProbeVolume.surface_bias
#. :src: bpy.types.GREASE_PENCIL_OT_reproject.offset
#. :src: bpy.types.ToolSettings.gpencil_surface_offset
msgid "Surface Offset"
msgstr "Dịch Chuyển trong chế độ Bề Mặt"

