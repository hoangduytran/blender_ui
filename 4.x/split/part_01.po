#. :src: bpy.types.TRANSFORM_OT_edge_slide.use_even
#. :src: bpy.types.TRANSFORM_OT_vert_slide.use_even
#. :src: bpy.types.UV_OT_follow_active_quads.mode:'EVEN'
#: source/blender/editors/transform/transform_mode_edge_slide.cc:831
