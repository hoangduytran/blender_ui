msgstr "Khung hình cuối cùng để nướng mô phỏng đại dương"

#. :src: bpy.types.OceanModifier.frame_start
msgid "Bake Start"
msgstr "Bắt Đầu Nướng"

#. :src: bpy.types.OceanModifier.frame_start
