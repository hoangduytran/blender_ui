#. :src: bpy.types.MultiresModifier.uv_smooth
#. :src: bpy.types.SubsurfModifier.uv_smooth
#. :src: bpy.types.GeometryNodeSubdivisionSurface.uv_smooth
msgid "UV Smooth"
msgstr "Cách Làm Mịn UV"

#. :src: bpy.types.MultiresModifier.uv_smooth
#. :src: bpy.types.SubsurfModifier.uv_smooth
#. :src: bpy.types.GeometryNodeSubdivisionSurface.uv_smooth
msgid "Controls how smoothing is applied to UVs"
msgstr "Điều khiển phương pháp làm mịn áp dụng vào các UV"

#. :src: bpy.types.MultiresModifier.uv_smooth:'NONE'
#. :src: bpy.types.SubsurfModifier.uv_smooth:'NONE'
#. :src: bpy.types.GeometryNodeSubdivisionSurface.uv_smooth:'NONE'
msgid "UVs are not smoothed, boundaries are kept sharp"
msgstr "UV sẽ không được làm mịn, các đường ranh giới sẽ duy trì độ sắc nhọn"

#. :src: bpy.types.MultiresModifier.uv_smooth:'PRESERVE_CORNERS'
#. :src: bpy.types.SubsurfModifier.uv_smooth:'PRESERVE_CORNERS'
#. :src: bpy.types.GeometryNodeSubdivisionSurface.uv_smooth:'PRESERVE_CORNERS'
msgid "UVs are smoothed, corners on discontinuous boundary are kept sharp"
msgstr "UV sẽ được làm mịn, các góc và các đường ranh giới gián đoạn sẽ được duy trì độ sắc nhọn"

#. :src: bpy.types.MultiresModifier.uv_smooth:'PRESERVE_CORNERS_AND_JUNCTIONS'
#. :src: bpy.types.SubsurfModifier.uv_smooth:'PRESERVE_CORNERS_AND_JUNCTIONS'
#. :src: bpy.types.GeometryNodeSubdivisionSurface.uv_smooth:'PRESERVE_CORNERS_AND_JUNCTIONS'
msgid "Keep Corners, Junctions"
msgstr "Duy Trì Góc Cạnh, Mối Nối"

#. :src: bpy.types.MultiresModifier.uv_smooth:'PRESERVE_CORNERS_AND_JUNCTIONS'
#. :src: bpy.types.SubsurfModifier.uv_smooth:'PRESERVE_CORNERS_AND_JUNCTIONS'
#. :src: bpy.types.GeometryNodeSubdivisionSurface.uv_smooth:'PRESERVE_CORNERS_AND_JUNCTIONS'
msgid "UVs are smoothed, corners on discontinuous boundary and junctions of 3 or more regions are kept sharp"
msgstr "UV sẽ được làm mịn, các góc nằm trên các đường ranh giới gián đoạn và các vùng mối nối của 3 hoặc hơn sẽ được giữ là sắc nhọn"

#. :src: bpy.types.MultiresModifier.uv_smooth:'PRESERVE_CORNERS_JUNCTIONS_AND_CONCAVE'
#. :src: bpy.types.SubsurfModifier.uv_smooth:'PRESERVE_CORNERS_JUNCTIONS_AND_CONCAVE'
#. :src: bpy.types.GeometryNodeSubdivisionSurface.uv_smooth:'PRESERVE_CORNERS_JUNCTIONS_AND_CONCAVE'
msgid "Keep Corners, Junctions, Concave"
msgstr "Duy Trì Góc Cạnh, Mối Nối, Chỗ Lõm Vào"

#. :src: bpy.types.MultiresModifier.uv_smooth:'PRESERVE_CORNERS_JUNCTIONS_AND_CONCAVE'
#. :src: bpy.types.SubsurfModifier.uv_smooth:'PRESERVE_CORNERS_JUNCTIONS_AND_CONCAVE'
#. :src: bpy.types.GeometryNodeSubdivisionSurface.uv_smooth:'PRESERVE_CORNERS_JUNCTIONS_AND_CONCAVE'
msgid "UVs are smoothed, corners on discontinuous boundary, junctions of 3 or more regions and darts and concave corners are kept sharp"
msgstr "UV sẽ được làm mịn, các góc nằm trên các đường ranh giới gián đoạn, và các vùng mối nối của 3 hoặc hơn, các góc lồi và lõm sẽ được giữ là sắc nhọn"

#. :src: bpy.types.MultiresModifier.uv_smooth:'PRESERVE_BOUNDARIES'
#. :src: bpy.types.SubsurfModifier.uv_smooth:'PRESERVE_BOUNDARIES'
#. :src: bpy.types.GeometryNodeSubdivisionSurface.uv_smooth:'PRESERVE_BOUNDARIES'
#: source/blender/nodes/geometry/nodes/node_geo_dual_mesh.cc:21
msgid "Keep Boundaries"
msgstr "Duy Trì Ranh Giới"

#. :src: bpy.types.MultiresModifier.uv_smooth:'PRESERVE_BOUNDARIES'
#. :src: bpy.types.SubsurfModifier.uv_smooth:'PRESERVE_BOUNDARIES'
#. :src: bpy.types.GeometryNodeSubdivisionSurface.uv_smooth:'PRESERVE_BOUNDARIES'
msgid "UVs are smoothed, boundaries are kept sharp"
msgstr "UV sẽ được làm mịn, các đường ranh giới sẽ được giữ là sắc nhọn"

#. :src: bpy.types.MultiresModifier.uv_smooth:'SMOOTH_ALL'
#. :src: bpy.types.SubsurfModifier.uv_smooth:'SMOOTH_ALL'
#. :src: bpy.types.GeometryNodeSubdivisionSurface.uv_smooth:'SMOOTH_ALL'
msgid "UVs and boundaries are smoothed"
msgstr "Các UV và các ranh giới đều được làm mịn"

