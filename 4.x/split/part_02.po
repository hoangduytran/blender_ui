msgid "Prefers to connect foreground components"
msgstr "Thích kết nối các thành phần tiền cảnh"

#. :src: bpy.types.GREASE_PENCIL_OT_trace_image.turnpolicy:'BACKGROUND'
msgid "Prefers to connect background components"
msgstr "Thích kết nối các thành phần nền sau"

#. :src: bpy.types.GREASE_PENCIL_OT_trace_image.turnpolicy:'LEFT'
msgid "Always take a left turn"
msgstr "Luôn luôn rẽ trái"

#. :src: bpy.types.GREASE_PENCIL_OT_trace_image.turnpolicy:'RIGHT'
msgid "Always take a right turn"
msgstr "Luôn luôn rẽ phải"

#. :src: bpy.types.GREASE_PENCIL_OT_trace_image.turnpolicy:'MINORITY'
msgid "Minority"
msgstr "Thiểu Số"

#. :src: bpy.types.GREASE_PENCIL_OT_trace_image.turnpolicy:'MINORITY'
msgid "Prefers to connect the color that occurs least frequently in the local neighborhood of the current position"
msgstr "Thích kết nối màu ít xuất hiện nhất trong vùng lân cận của vị trí hiện tại"

#. :src: bpy.types.GREASE_PENCIL_OT_trace_image.turnpolicy:'MAJORITY'
msgid "Majority"
msgstr "Đa Số"

#. :src: bpy.types.GREASE_PENCIL_OT_trace_image.turnpolicy:'MAJORITY'
msgid "Prefers to connect the color that occurs most frequently in the local neighborhood of the current position"
