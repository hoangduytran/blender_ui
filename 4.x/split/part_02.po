#. :src: bpy.types.SceneEEVEE.shadow_pool_size:'1024'
msgid "1 GB"
msgstr ""

#. :src: bpy.types.SceneEEVEE.gi_visibility_resolution
msgid "Irradiance Visibility Size"
msgstr "Kích Thước Tầm Nhìn của Sự Tỏa Sáng"

#. :src: bpy.types.SceneEEVEE.gi_visibility_resolution
msgid "Size of the shadow map applied to each irradiance sample"
msgstr "Kích thước của ánh xạ bóng tối áp dụng vào mỗi mẫu vật của sự tỏa sáng"

#. :src: bpy.types.SceneEEVEE.gi_visibility_resolution:'8'
msgid "8 px"
msgstr "8 điểm ảnh"

#. :src: bpy.types.SceneEEVEE.gi_visibility_resolution:'16'
msgid "16 px"
msgstr "16 điểm ảnh"

#. :src: bpy.types.SceneEEVEE.gi_visibility_resolution:'32'
msgid "32 px"
